<h1 class="left"><%= t('parser_templates.label') %></h1>
<%= link_to t('parser_templates.create'), new_parser_template_path, class: "button new-right" %>

<table class="twelve">
  <thead>
    <tr>
      <th><%= t('name') %></th>
    </tr>
  </thead>

  <tbody>
    <% @parser_templates.each do |parser_template| %>
    <tr>
      <td><%= link_to parser_template.name, edit_parser_template_path(parser_template) %></td>
    </tr>
    <% end %>
  </tbody>
</table>